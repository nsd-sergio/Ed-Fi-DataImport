                    }
                ]
            }
        },
        "WriteTo:AppLog": {
            "Name": "Conditional",
            "Args": {
                "expression": "LogType is null",
                "configureSink": [
                    {
                        "Name": "PostgreSQL",
                        "Args": {
                            "connectionString": "defaultConnection",
                            "tableName": "ApplicationLogs",
                            "schemaName": "public",
                            "needAutoCreateTable": false,
                            "loggerColumnOptions": {
                                "Id": "IdAutoIncrement",
                                "Logged": "Timestamp",
                                "Properties": "LogEvent",
                                "Message": "RenderedMessage",
                                "Exception": "Exception"
                            },
                            "loggerPropertyColumnOptions": {
                                "MachineName": {
                                    "Name": "MachineName",
